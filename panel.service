[Unit]
Description=Simple XolPanel - @xlordeuyy
Documentation=https://t.me/sshxvpn
After=network.target

[Service]
User=root
WorkingDirectory=/root
ExecStart=/usr/bin/python3 -m xolpanel
Restart=on-failure
RestartPreventExitStatus=23
LimitNPROC=10000
LimitNOFILE=1000000

[Install]
WantedBy=multi-user.target
